@using Internal_set_a.Models
@model TBLDonor

        @using (Html.BeginForm("insert", "Donor", FormMethod.Post))
        {
            <label>name</label>
            @Html.TextBoxFor(a => a.Name);
            <label>Donation Date</label>
            @Html.TextBoxFor(a => a.Donation_Date, new { type = "Date" });
            <label>City</label>
            @Html.TextBoxFor(a => a.City);
            <label>Aadhar Number</label>
            @Html.TextBoxFor(a => a.Aadhar_Number);
            <label>Donation Amount</label>
            @Html.TextBoxFor(a => a.Donation_Amount);
            <input type="submit" value="submit" />
        }
    
